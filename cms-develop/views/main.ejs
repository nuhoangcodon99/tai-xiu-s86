<body>

    <!--*******************
        Preloader start
        
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    
        Preloader end
    ********************-->


    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        <div class="nav-header" style="background-color: #000000;">
            <div class="brand-logo">
                <a href="javascript:void(0)" onclick="callDataGame('home')">
                    <b class="logo-abbr"><img src="/images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <span class="text-danger" style="font-weight: 900;color: #ff9f7a !important;">YOUTUBE:</span>
                    <span class="text-info" style="font-weight: 900;color: #ebc022 !important;">SOURCE ALL</span>
                    </span>
                </a>
            </div>
        </div>
        <!--**********************************
            Nav header end
        ***********************************-->

        <!--**********************************
            Header start
        ***********************************-->
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
                <div class="header-left">
                </div>
                <div class="header-right">
                    <ul class="clearfix">
                        <li class="icons dropdown">
                            <a href="javascript:void(0)" onclick="callDataGame('home')">
                                <i class="fa fa-refresh"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

        <!--**********************************
            Sidebar start
        ***********************************-->
        <div class="nk-sidebar">
            <div class="nk-nav-scroll">
                <ul class="metismenu in" id="menu">
                </ul>
            </div>
        </div>
        <!--**********************************
            Sidebar end
        ***********************************-->

        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <!-- #/ container -->
            <div class="login-form-bg h-100" style="margin-top: 150px;" id="loginSence">
                <div class="container h-100">
                    <div class="row justify-content-center h-100">
                        <div class="col-xl-6">
                            <div class="form-input-content">
                                <div class="card login-form mb-0" style="border-radius: 2.625rem;">
                                    <div class="card-body pt-5">
                                        <div class="text-center">
                                            <img src="https://www.ionos.com/digitalguide/fileadmin/DigitalGuide/Teaser/cms-t.jpg" class="rounded-circle" style="height: 138px;box-shadow: 0px 0px 75px -8px #141c32;">
                                        </div>
                                        <div class="mt-5 mb-5 login-input">
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="username" placeholder="Tài khoản Admin" value="">
                                            </div>
                                            <div class="form-group">
                                                <input type="password" class="form-control" id="password" placeholder="Mật khẩu Admin" value="">
                                            </div>
                                            <div class="form-group">
                                                <div class="input-group">
                                                    <input type="number" id="otpcode" class="form-control" placeholder="Nhập Mã OTP" min="1" max="6"> <span class="input-group-append" style="cursor: -webkit-grab; cursor: grab;" onclick="loginGetOTP()"><span class="input-group-text">Lấy OTP</span></span>
                                                </div>
                                            </div>
                                            <button class="btn login-form__btn submit w-100" onclick="login()">Đăng
                                                nhập</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <script>
                    $(document).keypress(function(event) {
                        var keycode = (event.keyCode ? event.keyCode : event.which);
                        if (keycode == '13') login();
                    });
                </script>
            </div>
        </div>
    </div>
    <!--**********************************
                    Content body end
                ***********************************-->


    <!--**********************************
                    Footer start
                ***********************************-->
